#!/bin/bash

NAME=ohos

[ -r /etc/default/$NAME ] && . /etc/default/$NAME

if [[ "$MDNS_ACTIVE" == 'yes' ]]
then
        /usr/bin/custom-mdns $MDNS_ARGS;
fi

if [[ "$AUTOUPDATE_ACTIVE" == 'yes' ]]
then
	/usr/bin/auto-update $AUTOUPDATE_ARGS;
fi

if [[ "$ISCSI_CLIENT" == 'yes' ]]
then
	/usr/bin/iscsi-setup $ISCSI_ARGS;
fi